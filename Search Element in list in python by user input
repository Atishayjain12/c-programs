
l1=[]
k=0
c=1
n=int(input("Enter the range"))
print("Enter the elements of list",l1,"of range",n)
for i in range (0,n+1):
    ele = int(input(""))
    l1.append(ele)

print(l1)    

s = int(input("Enter the number to search"))
for i in l1:
    if (i == s):
        k+=1
        break
    c+=1
if (k==0):
  print("Not found") 
else:
  print("Found at position",c)
